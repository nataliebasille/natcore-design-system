{
  "name": "@nataliebasille/natcore-css-engine",
  "version": "1.0.0",
  "description": "CSS engine for Natcore Design System - DSL and transformers for CSS generation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "css",
    "dsl",
    "css-engine",
    "tailwindcss",
    "design-system"
  ],
  "author": "Natalie Basille",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.19.0",
    "@vitest/ui": "^2.1.8",
    "dedent": "^1.7.0",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "csstype": "^3.2.3"
  }
}
